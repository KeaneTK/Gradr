extends ./base.jade

block stylesheets
	link(href='/css/style.css', rel='stylesheet')

block scripts
	script(src='/js/matchList.js')


block content
		.container

			each match in matches
				ul.list-group
					li.list-group-item
						a(href="/MatchProfile?user=#{match.id}") !{match.firstname} !{match.lastname}